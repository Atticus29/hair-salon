<!DOCTYPE html>
<html>
  <head>
    <title>hair-salon</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="/app.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="/js/scripts.js"></script>
  </head>
  <body>
    <div class="jumbotron" id="master_header">
      <div class="container">
        <div class="row" id="search-row">
          <form action="ATTN" method="post" id="search-form">
            <label for="search-term">Search for stylist:</label>
            <input type="text" name="search-term" value="Naomi Smalls" class="control-form">
            <button type="submit" class="btn btn-preview">Search</button>
          </form>
        </div>
        <div class="row" id="image-and-title">
          <a href="/" id="home-image">
            <img src="/images/logo.jpg" alt="logo" id="logo-image">
          </a>
          <h1 id="main-title">
            Ru Paul's Hair Salon
          </h1>
        </div>
      </div>
    </div>
    <!-- begin inner template-->
    <div class="container">
      #parse( $template )
    </div>
    <!-- end inner template-->
  </body>
</html>
